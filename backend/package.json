{
    "name": "asset-transfer-basic",
    "version": "1.0.0",
    "description": "Asset Transfer Basic Application implemented in typeScript using fabric-gateway",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "dev": "nodemon --exec ts-node src/index.ts",
        "build": "tsc",
        "build:watch": "tsc -w",
        "lint": "eslint . --ext .ts",
        "prepare": "npm run build",
        "pretest": "npm run lint",
        "start": "node dist/app.js"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "@grpc/grpc-js": "~1.7.2",
        "@hyperledger/fabric-gateway": "^1.1.1",
        "axios": "^1.2.3",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "sqlite3": "^5.1.4",
        "typeorm": "^0.3.11",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@tsconfig/node14": "^1.0.3",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.15",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^14.18.16",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "@typescript-eslint/parser": "^5.22.0",
        "eslint": "^8.14.0",
        "typescript": "~4.6.4"
    }
}
